<h2>Listing Session Enrollments</h2>

<table class="table">
  <thead>
    <tr>
      <th>Coursesession</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessionenrolls.each do |sessionenroll| %>
      <tr>
        <td><%= sessionenroll.coursesession.name %></td>
        <td>
        <%= link_to 'Show', sessionenroll, class: "btn btn-primary" %>
        <%= link_to 'Edit', edit_sessionenroll_path(sessionenroll), class: "btn btn-success" %>
        <%= link_to 'Destroy', sessionenroll, method: :delete, data: { confirm: 'Are you sure?' },
                                                                          class: "btn btn-danger" %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Enrollment', new_sessionenroll_path, class: "btn btn-default" %>
